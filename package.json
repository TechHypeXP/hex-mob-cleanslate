{
  "name": "cleanslate-mobile",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start": "bunx expo start",
    "android": "bunx expo start --android",
    "ios": "bunx expo start --ios",
    "web": "bunx expo start --web",
    "build": "bunx expo export",
    "type-check": "bunx --bun tsc -p apps/mobile/tsconfig.json --noEmit",
    "lint": "bunx eslint . --ext .ts,.tsx --fix",
    "lint:fix": "bunx eslint . --ext .ts,.tsx --fix",
    "test": "bunx jest --passWithNoTests",
    "test:watch": "bun test --watch",
    "test:e2e": "bunx detox test",
    "prepare": "husky",
    "typedoc": "bunx typedoc",
    "docs:generate": "bunx typedoc && bunx code-narrator document",
    "health-check": "bun run type-check:mobile && bun run lint && bun run test && bun run build",
    "dev": "bunx expo start",
    "dev:reset": "./tools/scripts/dev-reset.sh",
    "dev:start": "./tools/scripts/dev-start.sh",
    "dev:mobile": "./tools/scripts/smart-start-v53.sh",
    "cleanup": "./tools/scripts/smart-cleanup-v53-conservative.sh",
    "type-check:mobile": "bunx --bun tsc -p apps/mobile/tsconfig.json --noEmit --skipLibCheck"
  },
  "dependencies": {
    "@eslint/compat": "^1.3.1",
    "@expo/vector-icons": "^14.1.0",
    "@react-navigation/bottom-tabs": "^7.4.4",
    "@react-navigation/native": "^7.1.16",
    "@reduxjs/toolkit": "^2.8.2",
    "@tanstack/react-query": "^5.83.1",
    "@types/react-redux": "^7.1.34",
    "expo": "~53.0.0",
    "expo-blur": "~14.1.5",
    "expo-file-system": "~18.1.11",
    "expo-haptics": "~14.1.4",
    "expo-linear-gradient": "~14.1.5",
    "expo-media-library": "~17.1.7",
    "expo-updates": "~0.28.17",
    "i18next": "^25.3.2",
    "lru-cache": "^11.1.0",
    "lucide-react-native": "^0.534.0",
    "react": "19.0.0",
    "react-i18next": "^15.6.1",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-vector-icons": "^10.0.0",
    "react-redux": "^9.2.0",
    "redux": "^5.0.1",
    "redux-thunk": "^3.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@eslint/eslintrc": "^3.3.1",
    "@expo/cli": "^0.17.0",
    "@expo/ngrok": "4.1.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.2.2",
    "@types/jest": "^30.0.0",
    "@types/marked": "^6.0.0",
    "@types/node": "^24.1.0",
    "@types/react": "~19.0.10",
    "@types/react-native": "^0.73.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "code-narrator": "^1.0.17",
    "detox": "^20.13.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-local-rules": "^3.0.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^16.1.2",
    "react-test-renderer": "19.0.0",
    "remark-lint": "^10.0.1",
    "typedoc": "^0.28.8",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.38.0"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "eslint --fix"
    ],
    "**/*.{md}": [
      "remark-lint"
    ]
  }
}
